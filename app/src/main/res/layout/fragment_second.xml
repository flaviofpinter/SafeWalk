<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#242323"
    tools:context=".SecondFragment">

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="337dp"
        android:layout_height="535dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginLeft="137dp"
        android:layout_marginTop="148dp"
        android:layout_marginEnd="24dp"
        android:layout_toEndOf="@+id/button_secondnext"
        android:background="@drawable/rectangle_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_secondnext" />

    <Button
        android:id="@+id/button_secondnext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="271dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="142dp"
        android:text="@string/next"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/rectangle_2"
        android:layout_marginEnd="17dp"
        android:text="@string/previous"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.736"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view2"
        android:layout_width="289dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignTop="@+id/rectangle_2"
        android:layout_marginStart="31dp"
        android:layout_marginTop="46dp"
        android:background="@drawable/retangulo_3" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="102dp"
        android:layout_height="33dp"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignTop="@+id/view2"
        android:layout_marginStart="45dp"
        android:layout_marginTop="14dp"
        android:text="@string/nome_da_rua"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignTop="@+id/rectangle_2"
        android:layout_marginStart="145dp"
        android:layout_marginTop="45dp"
        android:ems="10"
        android:inputType="text"
        android:text="Name" />

    <View
        android:id="@+id/view4"
        android:layout_width="289dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignTop="@+id/rectangle_2"
        android:layout_marginStart="35dp"
        android:layout_marginTop="125dp"
        android:background="@drawable/retangulo_3" />

    <View
        android:id="@+id/view5"
        android:layout_width="288dp"
        android:layout_height="39dp"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignTop="@+id/rectangle_2"
        android:layout_marginStart="39dp"
        android:layout_marginTop="208dp"
        android:background="@drawable/retangulo_3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignTop="@+id/view2"
        android:layout_marginStart="65dp"
        android:layout_marginTop="170dp"
        android:text="@string/incidente"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignTop="@+id/rectangle_2"
        android:layout_marginStart="137dp"
        android:layout_marginTop="124dp"
        android:ems="10"
        android:inputType="text"
        android:text="Name" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="106dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignTop="@+id/view2"
        android:layout_marginStart="52dp"
        android:layout_marginTop="94dp"
        android:text="@string/bairro"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="45dp"
        android:layout_marginTop="26dp"
        app:srcCompat="@drawable/usuario" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="165dp"
        android:layout_marginTop="773dp"
        android:text="Button" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="258dp"
        android:layout_height="104dp"
        android:layout_alignStart="@+id/rectangle_2"
        android:layout_alignBottom="@+id/rectangle_2"
        android:layout_marginStart="56dp"
        android:layout_marginBottom="180dp"
        android:spinnerMode="dropdown" />

</RelativeLayout>